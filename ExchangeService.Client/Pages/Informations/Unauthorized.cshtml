@using ExchangeService.Shared.Resources;
@using ExchangeService.Client.Services;
@page "/unauthorized"
@inject HttpClient Http
@inject TokenService tokenService
@inject Microsoft.AspNetCore.Blazor.Services.IUriHelper UriHelper


<div class="col-sm-7">
    <div class="alert alert-danger" role="alert">
        You do not have permision to visit this page!
        <br />
        <div class="row">
            <div class="col-sm-6">
                <a role="button" class="btn btn-light btn-block" href="/login">Login</a>
            </div>
            <div class="col-sm-6">
                <a role="button" class="btn btn-light btn-block" href="/register">Register</a>
            </div>
        </div>
    </div>
</div>
